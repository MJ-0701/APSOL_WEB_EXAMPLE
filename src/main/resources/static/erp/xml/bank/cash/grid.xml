<?xml version="1.0"?>
<rows>
	<head>
		<column id="no" width='50' align='center' type='ro' sort='na'>순 번
		</column>
		<column id="usedTime" width='120' align='center' type='ro' sort='na'>사용일시
		</column> 
		<column id="tradingKindName" width='100' align='center' type='ro' sort='na'>거래구분명
		</column>
		<column id="name" width='80' align='center' type='ro' sort='na'>가맹점명
		</column>
		<column id="businessNumber" width='100' align='center' type='ro' sort='na'>가맹점 사업자 번호
		</column>
		
		<column id="total" width='100' align='right' type='ron' sort='na' format="0,000">합계금액
		</column>
		
		<column id="amount" width='100' align='right' type='ron' sort='na' format="0,000">공급가액
		</column>
		
		<column id="tax" width='100' align='right' type='ron' sort='na' format="0,000">부가세
		</column>
		
		<column id="tip" width='120' align='right' type='ron' sort='na' format="0,000">봉사료
		</column>
		
		<column id="authNo" width='120' align='center' type='ro' sort='na'>승인번호
		</column>
				
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
				<param>50</param>
			</call>
		</afterInit>

	</head>
</rows>