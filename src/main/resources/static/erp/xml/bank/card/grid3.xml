		<column id="etcAmount" width='120' align='right' type='ron' sort='na' format="0,000">기타금액
		</column>
		
		<column id="depositAmount" width='120' align='right' type='ron' sort='na' format="0,000">입금금액
		</column>
				
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
				<param>50</param>
			</call>
		</afterInit>

	</head>
