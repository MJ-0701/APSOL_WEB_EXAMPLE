<?xml version="1.0"?>
<rows>
	<head>
		<column id="no" width='40' align='left' type='ro' sort='str' color=''></column>
		<column id="name" width='140' align='left' type='ro' sort='str' color=''>품목 </column>
		<column id="barcode" width='110' align='left' type='ro' sort='str' color=''>#cspan</column>
		<column id="serialNumber" width='100' align='left' type='ro' sort='str' color=''>#cspan</column>
		<column id="profit" width='100' align='center' type='ro' sort='str' color=''>손익확정</column>
		<column id="date" width='100' align='center' type='ro' sort='str' color=''>일 자</column>
		<column id="kindName" width='70' align='center' type='ro' sort='str' color=''>구 분</column>
		<column id="typeName" width='70' align='center' type='ro' sort='str' color=''>업 무</column>
		<column id="qty" width='70' align='right' type='ron' sort='int' color=''>수 량</column>
		<column id="memo" width='250' align='left' type='ro' sort='na' color=''>사 유</column>
		<column id="sender" width='70' align='center' type='ro' sort='str' color=''>요청자</column>
		<column id="managerName" width='70' align='center' type='ro' sort='str' color=''>확정자</column>
		<beforeInit>
			<call command="attachHeader">
				<param>,품목명,바코드,시리얼넘버,#rspan,#rspan,#rspan,#rspan,#rspan,#rspan,#rspan,#rspan</param>
			</call>
		</beforeInit>
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
			</call>
		</afterInit>

	</head>
</rows>