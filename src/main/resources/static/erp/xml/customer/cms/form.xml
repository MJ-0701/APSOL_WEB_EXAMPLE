						inputWidth="120" labelWidth="100" connector="cms/selectChargeDate" />
				</item>


				<item type="block" blockOffset="0">
					<item type="select" label="고유번호구분" name="identityKind"
						inputWidth="120" labelWidth="100">
						<option value="J" text="1.생년월일" />
						<option value="B" text="2.사업자번호" />
