								className="form_readonly" />
							<item type="newcolumn" />
							<item type="input" value="" label="" name="bankName"
								inputWidth="76" labelWidth="70" />
								<item type="newcolumn" />
								<item type="input" value="" label="예금주" name="name"
								inputWidth="120" labelWidth="100" />
						</item>
						<item type="block" blockOffset="0">
							
