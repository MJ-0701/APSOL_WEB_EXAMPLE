<?xml version="1.0"?>
<rows>
	<head>
		<column id="itemKindName" width='40' align='center' type='ro' sort='str'>구 분</column>
		<column id="item" width='60' align='center' type='ro' sort='str' color=''>품 목</column>
		<column id="name" width='110' align='left' type='ed' sort='str' color=''>#cspan</column>
		<column id="standard" width='100' align='left' type='ed' sort='str' color=''>규 격</column>
		<column id="unitName" width='60' align='left' type='ed' sort='int' color=''>단 위</column>	
		<column id="taxType" width='65' align='center' type='combo' sort='str' editable="false"  xmlcontent="1">
			세금구분
			<option value="TX0001">1.별 도</option>
			<option value="TX0002">2.포 함</option>
			<option value="TX0003">3.없 음</option>
		</column>
		<column id="qty" width='50' align='right' type='edn' sort='int' color=''>수량</column>		
		<column id="unitPriceS" width='70' align='right' type='edn' sort='int' color=''>단가</column>
		<column id="amountS" width='80' align='right' type='edn' sort='int' color=''>합 계</column>
		<column id="amount" width='80' align='right' type='ron' sort='int' color=''>견적 금액</column>
		<column id="tax" width='70' align='right' type='ron' sort='int' color=''>#cspan</column>
		<column id="total" width='90' align='right' type='ron' sort='int' color=''>#cspan</column>
		<column id="unitPrice" width='70' align='right' type='ron' sort='int' color=''>견적 단가</column>
		<column id="memo" width='220' align='left' type='ed'	 sort='na' color=''>비 고</column>
		<beforeInit>
			<call command="attachHeader">
				<param>#rspan,코 드,품목명,#rspan,#rspan,#rspan,#rspan,#rspan,#rspan,금 액,세 액,합 계,#rspan,#rspan</param>
			</call>
		</beforeInit>
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
			</call>
			<call command="enableEditTabOnly">
				<param>1</param>
			</call>
			<call command="enableMultiselect">
				<param>1</param>
			</call>
			<call command="enableAccessKeyMap">
				<param>1</param>
			</call>
		</afterInit>
	</head>

</rows>