<?xml version="1.0"?>
<rows>
	<head>
	
		<column id="name" width='150' align='left' type='ro' sort='na' color=''>대리점(가맹점)명</column>
		<column id="point" width='100' align='right' type='ro' sort='na' color=''>현재포인트</column>
		<column id="point" width='100' align='right' type='ro' sort='na' color=''>사용포인트</column>
		<column id="usage" width='100' align='right' type='ro' sort='na' color=''><![CDATA[ 총사용금액<br/>(현금 + 카드) ]]></column>
		<column id="countCash" width='70' align='right' type='ro' sort='na' color=''>현금</column>
		<column id="amountCash" width='90' align='right' type='ro' sort='str' color=''>#cspan</column>
		<column id="countCard" width='70' align='right' type='ro' sort='na' color=''>카드</column>
		<column id="amountCard" width='90' align='right' type='ro' sort='str' color=''>#cspan</column>
		<column id="countSms" width='70' align='right' type='ro' sort='na' color=''>문자사용량</column>
		<column id="countLms" width='70' align='right' type='ro' sort='str' color=''>#cspan</column>
		<column id="tax" width='70' align='right' type='ro' sort='str' color=''>계산서</column>

		<beforeInit>
			<call command="attachHeader">
				<param>#rspan,#rspan,#rspan,#rspan,건수,금액,건수,금액,SMS 건수,LMS 건수,건수,</param>
			</call>
		</beforeInit>
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
				<param>50</param>
			</call>
		</afterInit>
	</head>
</rows>