		<column id="completeWithdrawDate" width='180' align='center' type='ro'
		sort='server'>탈퇴완료일시</column>
		
		
		<beforeInit>
			<call command="attachHeader">
				<param>#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,#text_filter,</param>
			</call>
		</beforeInit>
		<afterInit>
			<call command="enableSmartRendering">
				<param>1</param>
				<param>50</param>
			</call>

		</afterInit>

	</head>
